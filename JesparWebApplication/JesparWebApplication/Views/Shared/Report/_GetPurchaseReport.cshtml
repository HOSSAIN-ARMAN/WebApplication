<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0" style="color:black ;background-color:white">
    <tr>
       
        <th>Code</th>
        <th>Name</th>
        <th>Category</th>
        <th>Available Qty.</th>
        <th>Unit Price</th>
        <th>MRP</th>
        <th>Profit</th>
       


    </tr>
    @{
        var rep = ViewBag.x;
    }
   
    @foreach (var p in rep)
    {
        <tr>

           
            <td>@p.Code</td>
            <td>@p.Name</td>
            <td>@p.Category</td>
            <td>@p.AvailableQuantity</td>
            <td>@p.CostPrice</td>
            <td>@p.MRP</td>
            <td>@p.Profit</td>
 

        </tr>
    }

</table>
